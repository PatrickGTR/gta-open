static
	Float:MoneyBagX,
    Float:MoneyBagY,
    Float:MoneyBagZ,
	bool:HasMoneyBagBeenFound = false,
	MoneyBagPickupHandler = -1;

#define MONEYBAG_MIN_PRIZE		(75_000)
#define MONEYBAG_MAX_PRIZE		(150_000)

#include <YSI_Coding\y_hooks>

hook OnGameModeInit()
{
	// We initialise the money bag variable to true
    // to generate a money bag.
	HasMoneyBagBeenFound = true;

	// We initialise the money bag function, if the variable is set to true
	// we create the money bag position x, y, z and its location.
	OnMoneyBagUpdate();
    return 1;
}

hook OnPlayerPickUpDynPickup(playerid, pickupid) {
	if(pickupid == MoneyBagPickupHandler) {
		new
			prize = random(MONEYBAG_MAX_PRIZE - MONEYBAG_MIN_PRIZE) + MONEYBAG_MIN_PRIZE,
			location[MAX_MAP_ZONE_NAME],
            MapZone:zoneid = GetMapZoneAtPoint(MoneyBagX, MoneyBagY, MoneyBagZ);

        GetMapZoneName(zoneid, location, sizeof(location));

        // Notify all
        MessageBox_ShowAllF(
            TYPE_INFORMATION,
            "~y~Moneybag found!",
            "~y~WINNER WINNER CHICKEN DINNER! ~p~%p ~w~found the moneybag containing ~g~%m ~w~in ~p~%s~w~!",
            _,
            playerid, prize, location
        );

        // Notify player
        SendMsgF(playerid, COLOR_GREEN, "Congratulations! "C_WHITE"You found the "C_GOLD"money bag "C_WHITE"containing "C_GREEN"%m "C_WHITE"in "C_GREY"%s", prize, location);


        // Regenerate a new one.
		DestroyDynamicPickup(MoneyBagPickupHandler);
        HasMoneyBagBeenFound = true;
        OnMoneyBagUpdate();

        // Reward Player.
		GivePlayerMoney(playerid, prize);

        // Pickup animation.
		ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 4.0, false, false, false, false, 0, SYNC_ALL);
	}
    return 1;
}

// 12 minutes.
task OnMoneyBagUpdate[1000 * 60 * 12]() {
    new
		moneybaglocation[MAX_MAP_ZONE_NAME];

	if(!HasMoneyBagBeenFound) {
        new
            MapZone:zoneidmoney = GetMapZoneAtPoint(MoneyBagX, MoneyBagY, MoneyBagZ);

        GetMapZoneName(zoneidmoney, moneybaglocation, sizeof(moneybaglocation));

		MessageBox_ShowAllF(
            TYPE_INFORMATION,
            "~y~Moneybag hint!",
            "~w~The money bag has not been found and it is still hidden near ~p~%s",
            _,
            moneybaglocation
        );
	}
	else {
	    HasMoneyBagBeenFound = false;
	    new randombag = random(sizeof(MoneyBagData));
	    MoneyBagX = MoneyBagData[randombag][E_MONEYBAG_X];
	    MoneyBagY = MoneyBagData[randombag][E_MONEYBAG_Y];
	    MoneyBagZ = MoneyBagData[randombag][E_MONEYBAG_Z];

        new
            MapZone:zoneidmoney = GetMapZoneAtPoint(MoneyBagX, MoneyBagY, MoneyBagZ);

        GetMapZoneName(zoneidmoney, moneybaglocation, sizeof(moneybaglocation));

		defer MoneyBag_NotifyPlayers();

		MoneyBagPickupHandler = CreateDynamicPickup(1550, 2, MoneyBagX, MoneyBagY, MoneyBagZ);
	}
	return 1;
}

// This gives the player who found the money bag a moment of fame before
// showing the new location of where the new money bag location is.
timer MoneyBag_NotifyPlayers[7000]() {
	new
		moneybaglocation2[MAX_MAP_ZONE_NAME],
		MapZone:zoneid = GetMapZoneAtPoint(MoneyBagX, MoneyBagY, MoneyBagZ);

	GetMapZoneName(zoneid, moneybaglocation2, sizeof(moneybaglocation2));

	MessageBox_ShowAllF(
		TYPE_INFORMATION,
		"~y~Moneybag new location!",
		"~w~The money bag is hidden near ~p~%s",
		_,
		moneybaglocation2
	);
}
